    object QueryEdit: TMemo
      Left = 120
      Height = 502
      Top = 13
      Width = 938
      Anchors = [akTop, akLeft, akRight, akBottom]
      Font.Color = clWindowText
      Font.Height = -12
      Font.Name = 'Courier New'
      Lines.Strings = (
        'select to_char (log_time, ''YYYY-MM-DD"T"HH24:MI:SS.FF'') as Timestamp'
        '     , process_group_instance_id || '';'' || service_message_id || '';'' || user_task_id as MessageId'
        '     , process_group_instance_id as PGID'
        '     , requester_id as ServiceRequestorId'
        '     , service_id as ServiceId '
        '     , event_type as EventType'
        '     , $EventData'
        '     , business_object_id as BusinessObjectId'
        '     , user_task_type as UserTaskType'
        '     , user_task_id as UserTaskId'
        '     , user_task_exec_bankcode'
        '     , user_task_exec_employee_id'
        '     , user_task_exec_employee_level'
        '     , error_flag'
        '     , component_name'
        '     , composite_name'
        '     , composite_instance_id'
        '     , instance_id'
        '     , flow_id'
        'from APP_LOG_DATA'
        'where to_char(log_time, ''YYYY-MM-DD:HH24'') between :Param1 and :Param1 || ''Z'''
        'and (:Param2 = ''%'' or process_group_instance_id = :Param2)'
        'and (:Param3 = ''%'' or business_object_id = :Param3)'
        'and (:Param4 = ''%'' or requester_id = :Param4)'
        'order by MessageId, log_time desc '
        ''
        '-- Essential to have specific MessageId''s because queried records are grouped as one Event per MessageId'
        '-- Such a group should contain both request and response for a servicecall'
        '-- The $EventData column is expanded since the Oracle driver does not deliver 4000+ width columns'
        '-- Some result columns are mapped to specific properties for technical reasons '
        '-- The assumption is made that the content of EventType is a valid Xml tagname'
        '-- Param1...Param4 available for where clause; reference according sql standard for embedded queries'
      )
      ParentFont = False
      ScrollBars = ssBoth
      TabOrder = 0
    end
