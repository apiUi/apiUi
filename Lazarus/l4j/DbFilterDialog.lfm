        '           process_group_instance_id || '';'' || service_message_id'
        '        end'
        '        ) MessageId as :ws.MessageId'
        '     , requester_id as :ws.ServiceRequestorId'
        '     , service_id as :ws.ServiceId '
        '     , event_type as :ws.EventType'
        '     , to_char(event_data) as event_data as :ws.EventData'
        '     , business_object_id as :ws.Dummy'
        '     , user_task_type as :ws.Dummy'
        '     , user_task_id as :ws.Dummy'
        '     , user_task_exec_bankcode as :ws.Dummy'
        '     , error_flag as :ws.Dummy'
        '     , component_name as :ws.Dummy'
        '     , composite_name as :ws.Dummy'
        'from APP_LOG_DATA'
        '//where to_char(log_time, ''YYYY-MM-DD:HH24'') between :ws.Param1 and :ws.Param1 || ''Z'''
        '//and (:ws.Param2 = ''*'' or process_group_instance_id = :ws.Param2)'
        '//and (:ws.Param3 = ''*'' or composite_name = :ws.Param3)'
        'where service_message_id = ''cgi://6meAdm8bpDbn8bn40v7b04sf'''
        'order by 2, 5 '
        ''
        '// essential to have specific MessageId''s because queried records are grouped as one Event per MessageId'
        '// some result columns mapped to specific internal properties are used internally. '
        '// Assumption is made that the content of EventType is a valid Xml tagname (rfc on ....)'
        '// map as many new columns to ws.Dummy ( as :ws.Dummy) as you like'
      )
      ParentFont = False
      ScrollBars = ssVertical
      TabOrder = 0
    end
