      Lines.Strings = (
        'select to_char(log_time,''YYYY-MM-DD"T"HH24:MM:SS"."FF'') as Timestamp as :ws.TimeStamp'
        '     , (case '
        '         when (service_message_id is null or service_message_id = ''Not applicable'')'
        '         then '
        '           process_group_instance_id || '';'' || to_char(log_time,''YYYY-MM-DD"T"HH24:MM:SS"."FF'')'
