
unit yamlScanner;

interface
uses CustScanner, Classes, yamlParser;

type TyamlScanner = class (TCustScanner)
published
  property OnNeedData: TOnNeedDataEvent read FOnNeedData write FOnNeedData;
  property OnOutput: TNotifyEvent read FOnOutput write FOnOutput;
  property OnEcho: TNotifyEvent read FOnEcho write FOnEcho;
  property OnDefaultAction: TNotifyEvent read FOnDefaultAction write FOnDefaultAction;
  property OnError: TOnScannerErrorEvent read FOnError write FOnError;
public
  function yylex: Integer; override;
end;

implementation
  uses SysUtils;

const InitState = 2;
const MidlState = 4;
const RighState = 6;



function TyamlScanner.yylex : Integer;

procedure yyaction ( yyruleno : Integer );
  (* local definitions: *)

begin
  (* actions: *)
  case yyruleno of
  1:
                              begin
                                Start (InitState);
                                return (_COMMENT);
                              end;
  2:
                              begin
                                Start (InitState);
                                return (_NEWLINE);
                              end;
  3:
                              begin
                                Start (MidlState);
                                return (_NAME);
                              end;
  4:
                              return (_INDENT);
  5:
                              return (_HYPHENINDENT);
  6:
                              return (_STRING);
  7:
                              return (_VALUE);
  8:
                              return (_PIPE);
  9:
                              return (_WHITESPACE);
  10:
                              begin
                                Start (InitState);
                                return (_NEWLINE);
                              end;
  11:
                              begin
                                Start (RighState);
                                return (_WHITESPACE);
                              end;
  12:
                              begin
                                Start (RighState);
                                return (_PIPE);
                              end;
  13:
                              begin
                                Start (InitState);
                                return (_WHITESPACE);
                              end;
  14:
                              return (_STRING);
  15:
                              begin
                                Start (InitState);
                                return (_NEWLINE);
                              end;
  16:
                              return (_PIPE);
  17:
                              return (_WHITESPACE);
  18:
                              return (_VALUE);

  end;
end(*yyaction*);

